generator koj {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Domain {
    id       Int    @id @default(autoincrement())
    domain   String @default("none")
    domainId Int    @default(0)
    status   String @default("disabled")

    @@map("domain")
}
